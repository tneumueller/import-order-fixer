{
    "include": [
        ".ts"
    ],
    "exclude": [
        "node_modules"
    ],
    "spacing": {
        "afterGroup": 0,
        "afterImports": 1
    },
    "order": [
        "angular",
        "vendor",
        "modules",
        "components",
        "app",
        "any"
    ],
    "types": {
        "angular": {
            "matches": [
                "@angular",
                "@ngrx",
                "rxjs"
            ]
        },
        "app": {
            "matches": [
                "^app\\/.*"
            ]
        },
        "vendor": {
            "matches": [
                "^[^.]"
            ]
        },
        "modules": {
            "matches": [
                "..?\\/.*\\.module(.(ts|js))?"
            ]
        },
        "components": {
            "matches": [
                "..?\\/.*\\.component(.(ts|js))?"
            ]
        },
        "any": {
            "matches": [
                ".*"
            ]
        }
    }
}
